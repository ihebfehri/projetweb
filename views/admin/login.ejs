<div class="container center" ng-controller="AdminLogInController">
	<div class="row">
		<div class="col s2 m2 l3"><p></p></div>

    	<div class="col s8 m8 l6 card-panel white blue-text">
    		<form>
	    		<h5>Connexion Admin</h5></br>

				<div class="input-field col s12">
	    			<input id="email" type="text" ng-model="email" class="validate">
	      			<label class="active" for="email">Adresse e-mail</label>
	      		</div>

	      		<div class="input-field col s12">
	    			<input id="password" type="password" ng-model="password" class="validate">
	      			<label class="active" for="password">Mot de passe</label>
	      		</div>

	      		<div ng-if="errors.length > 0" class="left-align red-text">
	      			<ul>
	      				<li style="margin-left: 10px;" ng-repeat="error in errors">{{error}}</li>
	      			</ul>
	      		</div>

	      		<div class="center-align">
	      			<button class="waves-effect waves-light btn red" ng-click="LogIn();"><i class="material-icons right">done</i>GO</button>
	      		</div>
	      	</form>
	    	</br>
    	</div>

    	<div class="col s2 m2 l3"><p></p></div>
</div>